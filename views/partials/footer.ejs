<script>
    document.addEventListener("DOMContentLoaded", function() {
        // Get all the nav links
        const links = document.querySelectorAll(".nav-link");
        
        links.forEach(link => {
            // Remove 'active' class from all links initially
            link.classList.remove('active');
            
            // Compare the href of the link with the current URL's path
            if (window.location.pathname === link.getAttribute('href')) {
                link.classList.add('active'); // Add the 'active' class to the matching link
                link.setAttribute('aria-current', 'page'); // Add aria-current for accessibility
            } else {
                link.removeAttribute('aria-current'); // Remove aria-current if it's not the active link
            }
        });
    });
</script>

